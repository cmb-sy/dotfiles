# -------------------------------------------------------------------
# Customize Zsh's behavior to enable efficient command
# -------------------------------------------------------------------
setopt hist_ignore_dups
setopt hist_no_store
setopt share_history
setopt auto_list
setopt auto_menu


# -------------------------------------------------------------------
# Load sheldon
# -------------------------------------------------------------------
# eval "$(sheldon source)"

# -------------------------------------------------------------------
# Load functions
# -------------------------------------------------------------------
source ${HOME}/.functions.zsh

# -------------------------------------------------------------------
# Load aliases
# -------------------------------------------------------------------
source ${HOME}/.aliases
